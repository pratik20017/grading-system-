#include<stdio.h>
int main()
{
	int marks=0;
	printf("enter the marks scored by the student it should be less than 100 and greater than 0 \n",marks);
	scanf("%d",&marks);
	if(marks<=100 && marks>=85)
	{
		printf("grade A");
	}
	else if(marks<=84 && marks>=70)
	{
		printf("grade B");
	}
	else if(marks<=69 && marks>=55)
	{
		printf("grade C");
	}
	else if(marks<=54 && marks>=40)
	{
		printf("grade D");
	}
	else
	{
		printf("grade F");
	}
	return 0;
}
